        "\n",
        "        if openai == 'Fake':\n",
        "            print(\"This document may have been created by AI.\\n\")\n",
        "\n",
        "ml_client = MlClient(client)\n",
        "\n",
        "model_id = 'roberta-base-openai-detector' #open ai text classification model\n",
        "\n",
        "document = [\n",
