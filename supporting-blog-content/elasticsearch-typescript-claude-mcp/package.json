{
  "name": "elasticsearch-rag-mcp",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc index.ts --target ES2022 --module node16 --moduleResolution node16 --outDir ./dist --strict --esModuleInterop --skipLibCheck",
    "inspect": "npx @modelcontextprotocol/inspector",
    "start": "node dist/index.js",
    "dev": "node --loader ts-node/esm index.ts",
    "setup": "node --loader ts-node/esm setup.ts"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.1.1",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "openai": "^6.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
